          {
              for(int j=0;j<N;j++)
              {
                  cout<<Eg<<" ";
                  Eg++;
                  if(Eg == N+1) 
                    Eg = 1;
              }
              Eg--;
              if(Eg == 0) 
                Eg = N;
              cout<<endl;
          }
